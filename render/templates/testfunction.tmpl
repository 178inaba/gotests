			t.Errorf("%v. {{$f.Name}}() = %v, want %v", tt.name, got{{if $f.ReturnsMultiple}}{{$index}}{{end}}, tt.want{{if $f.ReturnsMultiple}}{{$index}}{{end}})
		}{{end}}{{end}}
	}
}
