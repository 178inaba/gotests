			t.Errorf("%v. {{$f.Name}}() = (want) -> (got)\n%v", tt.name, diff)
		}{{else}}{{if .IsScalar}}
		if {{if $f.OnlyReturnsOneValue}}got := {{template "func" $f}}; {{end}} got{{if $f.ReturnsMultiple}}{{$index}}{{end}} != tt.want{{if $f.ReturnsMultiple}}{{$index}}{{end}} { {{else}}
